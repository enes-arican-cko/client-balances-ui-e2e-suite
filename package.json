{
  "name": "dashboard-e2e-suite",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "npx playwright test",
    "sbx-test": "TEST_ENV=sbox npm run test",
    "show-report": "npx playwright show-report",
    "test:ui": "npx playwright test --ui",
    "sbx-test:ui": "TEST_ENV=sbox npm run test:ui",
    "test:headed": "npx playwright test --headed",
    "sbx-test:headed": "TEST_ENV=sbox npm run test:headed",
    "test:debug": "npx playwright test --debug",
    "sbx-test:debug": "TEST_ENV=sbox npm run test:debug",
    "test:trace": "npx playwright test --trace on",
    "sbx-test:trace": "TEST_ENV=sbox npm run test:trace",
    "test-with-report": "npx playwright test && npm run show-report",
    "sbx-test-with-report": "TEST_ENV=sbox npm run test-with-report",
    "test:visual": "npx playwright test 'tests/visual/*'",
    "sbx-test:visual": "TEST_ENV=sbox npm run test:visual"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justin-thomas-cko/dashboard-e2e-suite.git"
  },
  "homepage": "https://github.com/justin-thomas-cko/dashboard-e2e-suite#readme",
  "license": "ISC",
  "devDependencies": {
    "@estruyf/github-actions-reporter": "^1.3.0",
    "@playwright/test": "^1.41.1",
    "@types/node": "^20.10.6",
    "playwright-html": "^1.1.5",
    "winston": "^3.11.0"
  }
}
